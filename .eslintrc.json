{
  "env": {
    "es6": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-use-before-define": "off",
    "no-useless-constructor": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-undef":"off",
    "consistent-return": "off",
    "class-methods-use-this": "off",
    "camelcase": "off",
    "no-plusplus": "off",
    "radix": "off",
    "array-callback-return": "off",
    "no-console":"off",
    "arrow-parens":"off"
  }
}
